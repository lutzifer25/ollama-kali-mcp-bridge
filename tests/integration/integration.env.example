# Required: reachable Kali host/IP
KALI_HOST=192.168.178.70

# Optional: SSH user for Kali
KALI_USER=kali

# Optional: bridge config path
BRIDGE_CONFIG=bridge-config.example.json

# Optional: a target that nmap may scan (default: KALI_HOST)
KALI_TEST_TARGET=192.168.178.70

# Optional: timeout used inside tool calls
KALI_TEST_TIMEOUT_SEC=30

# Optional: max output bytes for MCP call
KALI_TEST_MAX_OUTPUT_BYTES=65536
